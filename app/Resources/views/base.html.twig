<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}{% endblock %}</title>
        {% stylesheets 'layout.sass' filter='compass'%}
            <link rel="stylesheet" type="text/css" media="screen" href="/sass{{ asset_url }}" />
        {% endstylesheets %}
        {% stylesheets '../ui/css/ui-lightness/jquery-ui-1.9.2.custom.css' '../fancybox/jquery.fancybox.css' %}
            <link rel="stylesheet" type="text/css" media="screen" href="/sass{{ asset_url }}" />
        {% endstylesheets %}
        {% javascripts '../ui/js/jquery-1.8.3.js' '../ui/js/jquery-ui-1.9.2.custom.min.js' '../js/selectBox.js' '../fancybox/jquery.fancybox.pack.js'  '../fancybox/jquery.fancybox.pack.js'%}
            <script type="text/javascript" src="/sass{{ asset_url }}"></script>
        {% endjavascripts %}
    </head>

    <body>
        {% block body %}

        {% endblock %}
    </body>
</html>